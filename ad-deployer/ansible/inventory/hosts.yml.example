---
# =============================================================================
# Inventaire Ansible pour AD-Deployer
# CE FICHIER EST GÉNÉRÉ AUTOMATIQUEMENT PAR deploy-ad.sh
# Ne pas modifier manuellement !
# =============================================================================

all:
  children:
    domain_controllers:
      hosts:
        dc01:
          ansible_host: <IP_CIBLE>
          
  vars:
    ansible_user: <ADMIN_USER>
    ansible_password: "<PASSWORD>"
    ansible_connection: winrm
    ansible_winrm_transport: ntlm
    ansible_port: 5985
    
    ad_domain:
      name: "<DOMAIN.LOCAL>"
      netbios: "<NETBIOS>"
      safe_mode_password: "<DSRM_PASSWORD>"
